<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user_path(user) %>
      <%= link_to "Edit", edit_user_path(user) %> |
      <%= button_to "Delete", user_path(user), method: :delete %>

    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>
